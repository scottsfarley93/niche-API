/sources:
  displayName: Data Sources
  get:
    description: Get a list of the models and data sources in the database
    queryParameters:
      producer:
        type: string
        example: NCAR
        required: false
        displayName: Data Producer
      model:
        type: string
        example: CCSM
        required: false
        displayName: Model
      modelVersion:
        type: number
        example: 3.0
        required: false
        displayName: Model Version
      modelScenario:
        type: string
        example: RCP8
        required: false
        displayName: Model Scenario
    responses:
      200:
        body:
          application/json:
            example: |
              {
                "message" : "",
                "success" : true,
                "status":200,
                "timestamp" : "12/19/2016, 7:00:00 PM",
                "data": [
                  {
                    "sourceID" : 237,
                    "producer" : "NCAR",
                    "model" : "CCSM",
                    "scenario":"",
                    "productVersion":3,
                    "productURL" : "http://www.cesm.ucar.edu/models/ccsm3.0/",
                    "lastUpdate" : "12/19/2016, 7:00:00 PM"
                  }
                ]
              }
  post:
    description: Add a new source or model data source to the database
    queryParameters:
      producer:
        type: string
        example: NCAR
        required: true
        displayName: Data Producer
      model:
        type: string
        example: CCSM
        required: true
        displayName: Model
      modelVersion:
        type: number
        example: 3.0
        required: true
        displayName: Model Version
      modelScenario:
        type: string
        example: RCP8
        required: true
        displayName: Model Scenario
      modelURL:
        type: string
        example: http://www.cesm.ucar.edu/models/ccsm3.0/,
        required: true
        displayName: Model URL
    responses:
      201:
        body:
          application/json:
            example: |
              {
                "message" : "Added source 238 to the database.",
                "success" : true,
                "status": 201,
                "timestamp" : "12/19/2016, 7:00:00 PM",
                "data": []
              }
