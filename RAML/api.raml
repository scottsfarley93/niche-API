/layers:
  description: Raster layers stored in the database
  displayName: Layers
  get:
    description: Get a list of the layers that are stored in the database
    queryParameters:
      yearsBP:
        displayName: Years B.P.
        description: Years before 1950
        required: false
        example: 22000
        type: integer
      variableType:
        displayName: Variable Type
        description: Variable type filter
        example: "Tmax"
        required: false
        type: string
      variablePeriod:
        displayName: VariablePeriod
        description: Variable period filter
        example: 1
        required: false
        type: integer
      variablePeriodType:
        displayName: Variable Period Name
        description: Variable period type specification for variablePeriod filter
        type: string
        example: Month
        required: false
      variableID:
        displayName: Variable ID
        description: Variable ID
        type: integer
        example: 13
        required: false
      sourceID:
        displayName: Data Source ID
        description: Data source ID number
        type: integer
        example: 13
        required: false
      sourceProducer:
        displayName: Data producer
        description: Entity that produced the data
        type: string
        example: NCAR
        required: false
      modelName:
        displayName: Model Name
        description: Name of the model that produced the data
        example: CCSM
        type: string
        required: false
      modelVersion:
        displayName: Model Version Number
        description: Version of the model that prduced the data
        example: 3
        type: number
        required: false
      resolution:
        displayName: Grid Resolution
        description: Native spatial resolution of the data in degrees
        example: 1
        type: number
        required: false
    responses:
      200:
        body:
          application/json:
            example: |
              {
              "success": true,
              "status": 200,
              "message": "",
              "timestamp" : "1/1/2000 5:17pm",
              "data": [
                {
                  "layerID" : 15,
                  "yearsBP":22000,
                  "variableID" : 22,
                  "variableType" : "Tmax"
                  "variableDescription" : "Decadal Average January Maximum Temperature"
                  "variablePeriod" : 1,
                  "VariablePeriodType" : "Month",
                  "AveragingPeriod" : 1,
                  "AveragingPeriodType" : "Decade",
                  "AveragingPeriodDays" : 3650,
                  "SourceID" : 2,
                  "DataProducer" : "NCAR",
                  "Model" :"CCSM",
                  "ModelVersion" : 3,
                  "TableName" : "ce7sf23wzs5d93"
                }
              ]
              }
  /{layerID}:
