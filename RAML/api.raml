#%RAML 1.0
title: Niche API
version: v1
baseUri: http://api.niche.geography.wisc.edu
/variables:
  displayName: Niche Variables
  get:
    description: Get a list of variables in the database
    queryParameters:
      variableType:
        displayName: Variable Type
        type: string
        example: TMax
        required: false
      variablePeriod:
        displayName: Variable Period
        type: integer
        example: 1
        required: false
      variablePeriodType:
        displayName: Variable Period Type
        type: string
        example: Month
        required: false
      averagingPeriod:
        displayName: Variable Averaging Period
        type: integer
        example: 1
        required: false
      averagingPeriodType:
        displayName: Variable Averaging Period Type
        example: Decade
        required: false
        type: string
      variableUnits:
        displayName: Variable Units
        example: cm
        required: false
        type: string
    responses:
      200:
        body:
          application/json:
            example: |
              {
                "success": true,
                "status":200,
                "timestamp": "12/19/2016, 7:00:00 PM",
                "data": [
                  {
                    "variableDescription": "Decadal Averaged January Mean Temperature",
                    "variablePeriod" : 1,
                    "variablePeriodType": "Month",
                    "averagingPeriod":1,
                    "averagingPeriodType":"Decade",
                    "variableUnits" : "C",
                    "variableID" : 1,
                    "lastUpdate" : "12/19/2016, 7:00:00 PM"
                  }
                ],
                "message": ""
              }
  post:
    description: Get a list of variables in the database
    queryParameters:
      variableType:
        displayName: Variable Type
        type: string
        example: prcp
        required: true
      variableDescription:
        displayName: Variable Description
        example: Decadal average total annual precipitation
        type: string
        required: true
      variableUnits:
        displayName: Variable Units
        example: cm
        type: string
        required: true
      variablePeriod:
        type: integer
        displayName: Variable Period
        example: 1
        required: true
      variablePeriodType:
        type: string
        displayName: Variable Period Type
        example: Year
        required: true
      averagingPeriod:
        type: integer
        displayName: Variable Averaging Period
        required: true
        example: 1
      averagingPeriodType:
        type: string
        displayName: Variable Averaging Period Type
        required: true
        example: Decade
    responses:
      201:
        body:
          application/json:
            example: |
              {
                "message" : "Added variable 231 to database.",
                "success" : true,
                "status" : 201,
                "timestamp" : "12/19/2016, 7:00:00 PM",
                "data" : []
              }
  /{variableid}:
    displayName: Niche Variables by ID
    get:
      description: Get details on a specific variable
      responses:
        200:
          body:
            application/json:
              example: |
                {
                "message" : "",
                "status":200,
                "success":true,
                "data" : [
                  {
                    "variableDescription": "Decadal Averaged January Mean Temperature",
                    "variablePeriod" : 1,
                    "variablePeriodType": "Month",
                    "averagingPeriod":1,
                    "averagingPeriodType":"Decade",
                    "variableUnits" : "C",
                    "variableID" : 1,
                    "lastUpdate" : "12/19/2016, 7:00:00 PM"
                  }
                ]
                }
    put:
      description: Update details of a specific variable with new information
      queryParameters:
        variableType:
          displayName: Variable Type
          type: string
          example: TMax
          required: false
        variablePeriod:
          displayName: Variable Period
          type: integer
          example: 1
          required: false
        variablePeriodType:
          displayName: Variable Period Type
          type: string
          example: Month
          required: false
        averagingPeriod:
          displayName: Variable Averaging Period
          type: integer
          example: 1
          required: false
        averagingPeriodType:
          displayName: Variable Averaging Period Type
          example: Decade
          required: false
          type: string
        variableUnits:
          displayName: Variable Units
          example: cm
          required: false
          type: string
      responses:
        200:
          body:
            application/json:
              example: |
                {
                  "message" : "Updated variable 231 to database.",
                  "success" : true,
                  "status" : 200,
                  "timestamp" : "12/19/2016, 7:00:00 PM",
                  "data" : [
                  {
                    "variableDescription": "Decadal Averaged January Mean Temperature",
                    "variablePeriod" : 1,
                    "variablePeriodType": "Month",
                    "averagingPeriod":1,
                    "averagingPeriodType":"Decade",
                    "variableUnits" : "C",
                    "variableID" : 1,
                    "lastUpdate" : "12/19/2016, 7:00:00 PM"
                  }
                  ]
                }

    delete:
      description: Delete a variable from the database
      responses:
        204:
          body:
            application/json:
              example: |
                {
                  "message" : "Deleted variable 231.",
                  "success" : true,
                  "status" : 200,
                  "timestamp" : "12/19/2016, 7:00:00 PM",
                  "data" : []
                }

