      description: Update specific source with new information
      queryParameters:
        producer:
          type: string
          example: NCAR
          required: false
          displayName: Data Producer
        model:
          type: string
          example: CCSM
          required: false
          displayName: Model
        modelVersion:
          type: number
          example: 3.0
          required: false
          displayName: Model Version
        modelScenario:
          type: string
          example: RCP8
          required: false
          displayName: Model Scenario
        modelURL:
          type: string
          example: http://www.cesm.ucar.edu/models/ccsm3.0/,
